    <file src="ITSM-Templates-WKS\Assets\Certificates\**\*.*" target="content\Certificates" />
    <file src="ITSM-Templates-WKS\Assets\MainDocs\**\*.*" target="content\MainDocs" />
    <file src="ITSM-Templates-WKS\BeforeJoinDomain\*.hta" target="tools\BeforeJoinDomain" />
