    <file src="Core-ScriptLibrary\Nuget-Package-Publisher\README.md" target="content\Nuget-Package-Publisher" />
    <file src="Core-ScriptLibrary\Modular-PS1-Scripts\README.md" target="content\Modular-PS1-Scripts" />
    <file src="Core-ScriptLibrary\README.md" target="content" />
